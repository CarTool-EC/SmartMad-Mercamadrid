      <property key="eira:view" value="OV-Data Spaces"/>
      <property key="eira:view" value="Organisational view"/>
      <property key="eira:eifLayer" value="Organisational"/>
      <property key="skos:broader" value="http://data.europa.eu/dr8/OrganisationalAgreementContract"/>
