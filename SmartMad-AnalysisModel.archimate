    <element xsi:type="archimate:Grouping" name="Business Agnostic Dataset Catalogue Solutions" id="id-221b4b9be65c485a9cdbd1ef1e710f40">
      <property key="eira:PURI" value="http://data.europa.eu/dr8/egovera/BusinessAgnosticDatasetCatalogueSolutionsGrouping"/>
      <property key="dct:modified" value="2023-11-20"/>
